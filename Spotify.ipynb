    "## 8. Genre-Based Audio Feature Analysis"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Audio features by genre - Radar chart\n",
    "if genre_col and normalized_features:\n",
    "    # Get top 10 genres\n",
    "    top_10_genres = df_clean[genre_col].value_counts().head(10).index\n",
    "    df_top_genres = df_clean[df_clean[genre_col].isin(top_10_genres)]\n",
    "    \n",
    "    # Create radar chart for each genre\n",
    "    features_for_radar = normalized_features[:6]  # Use first 6 features\n",
    "    \n",
    "    genre_means = df_top_genres.groupby(genre_col)[features_for_radar].mean()\n",
    "    \n",
    "    fig = go.Figure()\n",
    "    \n",
    "    for genre in genre_means.index:\n",
    "        fig.add_trace(go.Scatterpolar(\n",
    "            r=genre_means.loc[genre].values,\n",
    "            theta=[f.capitalize() for f in features_for_radar],\n",
    "            fill='toself',\n",
    "            name=genre\n",
    "        ))\n",
    "    \n",
    "    fig.update_layout(\n",
    "        polar=dict(\n",
    "            radialaxis=dict(\n",
    "                visible=True,\n",
    "                range=[0, 1]\n",
    "            )\n",
